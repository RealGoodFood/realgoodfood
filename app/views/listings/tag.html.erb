<h1><%= @tag %> Food</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Food category</th>
  </tr>

<% @results.each do |listing| %>
  <tr>
    <td><%=link_to listing.title, listing %></td>
    <td><%= link_to listing.food_category.name, listing.food_category %></td>
  </tr>
<% end %>
</table>

<br />

<h3><a href="/listings/new" class="button-save-cancel"><span>New Listing</span></a></h3>

