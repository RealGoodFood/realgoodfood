
    <div id="center-right-inner-container">
      <h1><%= @profile.full_name %></h1>
      
      <div>
          <p class="mb10">
           <a href="javascript:faq.sweepToggle('contract')" class="expandlink">Contract All</a> |
           <a href="javascript:faq.sweepToggle('expand')" class="expandlink">Expand All</a>
             <div class="eg-bar">
               <table width="99%" border="0" align="right" cellpadding="0" cellspacing="0">
                  <tr>
                    <td width="4%" align="left" valign="middle">
                     <span id="faq1-title" class="iconspan1">
                      <img src="/images/minus.gif" />
                     </span>
                    </td>
                    <td width="95%" align="left" valign="middle"> About your cooking interests</td>
                  </tr>
               </table>
          </div>
          <div style="display: block;" id="faq1" class="icongroup1">
              <%= @profile.about_me %>
          </div>
          <div class="eg-bar" "="">
             <table width="99%" border="0" align="right" cellpadding="0" cellspacing="0">
              <tr>
                <td width="4%" align="left" valign="middle"> <span id="faq2-title" class="iconspan1">
                  <img src="plus.gif" /></span> </td>
                <td width="95%" align="left" valign="middle"> About your favorite foods</td>
              </tr>
            </table>
             
       </div>
       <div style="display: none;" id="faq2" class="icongroup1">
          <% @profile.values.each do |f| %>
            <p><%= f.name %></p>
          <% end %>
       </div>
        
        <div class="eg-bar" "="">
        <table width="99%" border="0" align="right" cellpadding="0" cellspacing="0">
          <tr>
            <td width="4%" align="left" valign="middle"> <span id="faq3-title" class="iconspan1">
             <img src="plus.gif" /></span> </td>
            <td width="95%" align="left" valign="middle"> My location</td>
          </tr>
        </table>
        
       </div>
         <div style="display: none;" id="faq3" class="icongroup1"> 
          <%= @profile.location.address %>
         </div>
               
          <div class="eg-bar" "="">
        
           <table width="99%" border="0" align="right" cellpadding="0" cellspacing="0">
            <tr>
              <td width="4%" align="left" valign="middle"><span id="faq4-title" class="iconspan1">
                <img src="plus.gif" /></span> </td>
              <td width="95%" align="left" valign="middle">Social Network Profiles </td>
            </tr>
           </table>
        </div>
        <div style="display: none;" id="faq4" class="icongroup1"> 
  <p><%= image_tag "cell.jpeg",:size =>"20x30" %> : <strong><%= @profile.contact_number %></strong></p>
<br />

<b>Social Links</b>
 <p>

  <a href ="<%= @profile.facebook_profile_link %>"><%= image_tag "Facebook.png", :size => "30x30" %></a>
  <a href ="<%= @profile.twitter_profile_link %>"><%= image_tag "Twitter.png", :size => "30x30" %></a>
  <a href ="<%= @profile.linked_in_profile_link %>"><%= image_tag "LinkedIn.png", :size => "30x30" %></a>
  <a href ="<%= @profile.google_plus_profile_link %>"><%= image_tag "google_plus.jpeg", :size => "30x30" %></a>

</p>

        </div>
          <script type="text/javascript">

var faq=new switchicon("icongroup1", "div") //Limit scanning of switch contents to just "div" elements
faq.setHeader('<img src="/images/minus.gif" />', '<img src="/images/plus.gif" />') //set icon HTML
faq.collapsePrevious(true) //Allow only 1 content open at any time
faq.setPersist(false) //No persistence enabled
faq.defaultExpanded(0) //Set 1st content to be expanded by default
faq.init()

  </script>
          </p>
      </div>
      <br />
</table>
<br />


<h1><%= link_to 'Edit Profile', edit_profile_path(@profile) %>  </h1>


    </div>
  </div>

